a. int x = 'r';       //ok
b. char x = 123;      //ok
c. float x = 7,2;     //error
d. short x = 3L;      //error
e. double x = 0x123D; //ok
f. int x = 0x123F;    //ok
g. int x = 078;       //error
h. short x = 7;       //ok
i. byte x = 255;      //error
j. double x = 9;      //ok
k. int x = 5,5;       //error
l. int x = 5;         //ok